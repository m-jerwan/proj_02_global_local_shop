<p class="pageHeader">All Customers</p>

<div>
    <a href="/customers/new" class="button center">New Customer</a>
</div>



<table>
    <tr>
        <th> Name</th>
        <th> Address </th>
        <th></th>
    </tr>
    #foreach ($customer in $customers)
    <tr>
        <td>$customer.getCustomerName()</td>
        <td>$customer.getCustomerAddress()</td>
        <td><a href="/customers/$customer.getId()" class="button center">Details</a></td>
    </tr>
    #end
</table>
